{
  "name": "dynamic_novel",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:fix": "eslint src --ext .js,jsx,.ts,.tsx --fix",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-three/fiber": "^8.0.10",
    "@types/three": "^0.139.0",
    "eslint-config-prettier": "^8.5.0",
    "next": "12.1.4",
    "prettier": "^2.6.2",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "three": "^0.139.2"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.7.2",
    "@types/node": "17.0.23",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@typescript-eslint/typescript-estree": "4.6.0",
    "eslint": "^8.13.0",
    "eslint-config-next": "12.1.4",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "next-transpile-modules": "^9.0.0",
    "typescript": "4.6.3"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 120,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "semi": false,
    "trailingComma": "all",
    "arrowParens": "always",
    "endOfLine": "lf"
  }
}
